  "description": "Automated Instagram meme posting bot that scrapes copyright-safe memes from Pinterest and posts them with intelligent scheduling",
  "main": "src/index.js",
  "bin": {
    "insta-bot": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js run",
